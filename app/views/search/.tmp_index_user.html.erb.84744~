<h1> Search Users</h1>

<%= javascript_include_tag "prototype" %>

<%= form_tag({:action => 'search'})%>
<%= text_field_tag :searchuser %>
<% form_tag %>

<%= observe_field(:searchuser ,
				 :frequency => 0.5,
				  :update => :results,
				  :with => 'query',
				 :url => {:action => :live_search_user})%>
				 
<span id="results"> </span>
	 
